{
  "name": "@abstract-money/xion-onboard",
  "description": "Onboard any assets into XION",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "./src/applet.ts",
  "types": "./src/applet.ts",
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "typecheck": "tsc --noEmit",
    "dev": "vite",
    "generate:graz": "graz generate -g -M osmosis,noble,xion",
    "preinstall": "npx only-allow pnpm",
    "generate": "pnpm generate:graz",
    "postinstall": "pnpm generate",
    "build": "pnpm generate && NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "preview": "vite preview",
    "serve": "serve dist -s",
    "format": "biome format . --write",
    "lint": "biome check .",
    "lint:fix": "pnpm lint --apply"
  },
  "dependencies": {
    "@abstract-money/core": "^3.5.1",
    "@abstract-money/provider-graz": "^11.0.2",
    "@abstract-money/react": "^2.3.10",
    "@abstract-money/ui": "^0.0.1",
    "@burnt-labs/abstraxion": "1.0.0-alpha.54",
    "@cosmjs/amino": "^0.32.3",
    "@cosmjs/cosmwasm-stargate": "^0.32.3",
    "@cosmjs/encoding": "0.32.3",
    "@hookform/resolvers": "^3.3.4",
    "@julr/vite-plugin-validate-env": "^1.1.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@rainbow-me/rainbowkit": "^2.2.1",
    "@skip-go/client": "^0.16.3",
    "@tanstack/react-query": "^4.35.0",
    "@tanstack/react-query-devtools": "^5.62.8",
    "@tanstack/react-router": "^1.20",
    "@tanstack/react-table": "^8.13.2",
    "@tanstack/router-devtools": "1.15.19",
    "@unocss/reset": "^0.58.0",
    "alchemy-sdk": "^3.5.0",
    "bignumber.js": "^9.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cryptocurrency-icons": "^0.18.1",
    "graz": "^0.1.19",
    "is-ci": "^3.0.1",
    "lucide-react": "^0.299.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.50.1",
    "react-wrap-balancer": "^1.1.0",
    "tailwind-merge": "^2.1.0",
    "type-fest": "^4.8.3",
    "use-deep-compare": "^1.3.0",
    "usehooks-ts": "^2.9.4",
    "viem": "~2.21.36",
    "wagmi": "^2.13.5",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@abstract-money/cli": "^1.2.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tanstack/router-vite-plugin": "^1.8.2",
    "@types/is-ci": "^3.0.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "serve": "^14.2.0",
    "typescript": "^5.2.2",
    "unocss": "^0.58.0",
    "simple-git-hooks": "^2.9.0",
    "unocss-preset-animations": "^1.1.1",
    "unocss-preset-useful": "^0.9.2",
    "vite": "5.4.6",
    "vite-plugin-dts": "^3.7.2",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm format && pnpm lint:fix && git add -u"
  }
}
